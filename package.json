{
  "name": "parapasa",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "travis-ci": "ng lint  && ng build --aot --prod && ng test --watch=false --code-coverage --browsers HeadlessChrome && npm run e2e-chrome-headless",
    "prepush": "ng lint && ng build --aot --prod && ng test --watch=false --browsers HeadlessChrome && npm run e2e-chrome-headless",
    "e2e-chrome-headless": "ng e2e --protractor-config ./e2e/protractor-headless.conf.js",
    "source-map": "ng build --prod --source-map"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/material": "^6.4.7",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "~6.2.5",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^8.2.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.5",
    "@types/node": "~8.10.31",
    "codelyzer": "~4.5.0",
    "husky": "^1.1.2",
    "jasmine-core": "~3.2.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^1.3.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
