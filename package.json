{
  "name": "parapasa",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "travis-ci": "ng lint && ng test --code-coverage --browsers ChromeTravisCi && ng e2e && npm run build",
    "test-chrome-headless": "ng test --karma-config karma-headless.conf.js  --browsers HeadlessChrome",
    "e2e-chrome-headless": "ng e2e --protractor-config protractor-headless.conf.js",
    "prepush": "ng lint && ng build --aot --prod && ng test --watch=false  && ng e2e",
    "source-map": "ng build --prod --source-map"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.0.4",
    "@angular/cdk": "^6.2.1",
    "@angular/common": "6.0.4",
    "@angular/compiler": "6.0.4",
    "@angular/core": "6.0.4",
    "@angular/forms": "6.0.4",
    "@angular/http": "6.0.4",
    "@angular/material": "^6.2.1",
    "@angular/platform-browser": "6.0.4",
    "@angular/platform-browser-dynamic": "6.0.4",
    "@angular/router": "6.0.4",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.6",
    "rxjs": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "6.0.8",
    "@angular/compiler-cli": "6.0.4",
    "@angular/language-service": "6.0.4",
    "@types/jasmine": "^2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.3.2",
    "codelyzer": "^4.3.0",
    "husky": "^1.0.0-rc.9",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.2",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.1.0",
    "protractor": "^5.3.2",
    "ts-node": "~6.1.0",
    "tslint": "~5.10.0",
    "typescript": "2.7.2"
  }
}
